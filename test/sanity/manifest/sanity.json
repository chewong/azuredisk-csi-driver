{
    "apiVersion": "vlabs",
    "location": "westus2",
    "properties": {
        "orchestratorProfile": {
            "orchestratorType": "Kubernetes",
            "orchestratorRelease": "1.17"
        },
        "masterProfile": {
            "count": 1,
            "dnsPrefix": "{dnsPrefix}",
            "vmSize": "Standard_DS2_v2"
        },
        "agentPoolProfiles": [
            {
                "name": "agentpool1",
                "count": 1,
                "vmSize": "Standard_DS2_v2",
                "availabilityProfile": "AvailabilitySet",
                "storageProfile": "ManagedDisks"
            }
        ],
        "linuxProfile": {
            "adminUsername": "azureuser",
            "ssh": {
                "publicKeys": [
                    {
                        "keyData": "{keyData}"
                    }
                ]
            }
        },
        "servicePrincipalProfile": {
            "clientID": "{clientID}",
            "secret": "{secret}"
        }
    }
}
